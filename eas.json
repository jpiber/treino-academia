{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote",
    "requireCommit": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "EAS_NO_VCS": "1",
        "EXPO_NO_GIT_STATUS": "1"
      },
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

